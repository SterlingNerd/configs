{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "version": 2,
    "final_space": true,
    "console_title_template": "{{ .Shell }} in {{ .PWD }}",
    "palettes": {
        "template": "{{ if .WSL }}wsl{{ else }}{{ .Shell }}{{ end }}",
        "list": {
            "bash": {
                "foreground": "#00B4D8",
                "shell_foreground": "#007BFF",
                "shell_background": "#001B44",
                "battery_background": "#4CC9F0",
                "battery_foreground": "#251F44",
                "battery_charging": "#00D100",
                "battery_full": "#00B4D8",
                "battery_discharging": "#4895EF",
                "battery_low": "#FFA500",
                "battery_critical": "#FF0000",
                "path_background": "#F72585",
                "path_foreground": "#251F44",
                "git_foreground": "#004400",
                "git_background": "#00CC44",
                "git_changed": "#e2f292",
                "git_ahead_and_behind": "#ff4500",
                "git_ahead": "#B388FF",
                "git_behind": "#B388FF",
                "tooltip_foreground": "#251F44",
                "tooltip_background": "#4CC9F0"
            },
            "pwsh": {
                "foreground": "#007ACC",
                "shell_foreground": "#26C6DA",
                "shell_background": "#546E7A",
                "battery_background": "#a2beef",
                "battery_foreground": "#193549",
                "battery_charging": "#00D100",
                "battery_full": "#0476d0",
                "battery_discharging": "p:battery_background",
                "battery_low": "#FFCD58",
                "battery_critical": "#FF5588",
                "path_background": "#14c2dd",
                "path_foreground": "#193549",
                "git_foreground": "#193549",
                "git_background": "#a2c4e0",
                "git_changed": "#e2f292",
                "git_ahead_and_behind": "#ff4500",
                "git_ahead": "#B388FF",
                "git_behind": "#B388FF",
                "tooltip_foreground": "#193549",
                "tooltip_background": "#fffb38"
            },
            "wsl": {
                "foreground": "#E95420",
                "shell_foreground": "#E95420",
                "shell_background": "#5E2750",
                "battery_background": "#a2beef",
                "battery_foreground": "#FADADD",
                "battery_charging": "#00D100",
                "battery_full": "#0476d0",
                "battery_discharging": "p:battery_background",
                "battery_low": "#FFCD58",
                "battery_critical": "#FF5588",
                "path_background": "#772953",
                "path_foreground": "#FADADD",
                "git_foreground": "#300A24",
                "git_background": "#77216F",
                "git_changed": "#E95420",
                "git_ahead": "#E95420",
                "git_behind": "#E95420",
                "tooltip_foreground": "#300A24",
                "tooltip_background": "#77216F"
            }
        }
    },
    "transient_prompt": {
        "enabled": true,
        "timeout": 0,
        "foreground": "p:foreground",
        "style": "plain",
        "template": "\u279c ",
        "type": "text"
    },
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "newline": false,
            "segments": [
                {
                    "alias": "shell",
                    "foreground": "p:shell_foreground",
                    "background": "p:shell_background",
                    "leading_diamond": "\ue0c5",
                    "properties": {
                        "macos": "mac"
                    },
                    "style": "diamond",
                    "template": "{{.Icon}} \ue0b1",
                    "trailing_diamond": "\ue0b0",
                    "type": "os"
                },
                {
                    "type": "battery",
                    "template":
  " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295
  \ueb2d ","foreground": "p:battery_foreground","background": "p:battery_background",
                    "background_templates": [
  
              "{{if eq
  \"Charging\" .State.String}}p:battery_charging{{end}}",
  
              "{{if and (lt .Percentage 20) (eq
  \"Discharging\" .State.String)}}p:battery_critical{{end}}",
  
              "{{if and (lt .Percentage 50) (eq
  \"Discharging\" .State.String)}}p:battery_low{{end}}",
  
              "{{if eq
  \"Discharging\" .State.String}}p:battery_discharging{{end}}",
  
              "{{if eq
  \"Full\" .State.String}}p:battery_full{{end}}"
                    ],
                    "powerline_symbol": "\ue0b0",
                    "style": "powerline"
                },
                {
                    "type": "path",
                    "template": " \ue5ff {{ path .Path .Location }} ",
                    "background": "p:path_background",
                    "foreground": "p:path_foreground",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "style": "agnoster_short",
                        "max_depth": 4
                    },
                    "style": "powerline"
                },
                {
                    "type": "git",
                    "template":
  " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}
  \uf044 {
                        { .Working.String
                        }
                    }{
                        { end
                        }
                    }{
                        { if and (.Working.Changed) (.Staging.Changed)
                        }
                    } |{
                        { end
                        }
                    }{
                        { if .Staging.Changed
                        }
                    }
  \uf046 {
                        { .Staging.String
                        }
                    }{
                        { end
                        }
                    }{
                        { if gt .StashCount 0
                        }
                    }
  \ueb4b {
                        { .StashCount
                        }
                    }{
                        { end
                        }
                    } ","background": "p:git_background","background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git_changed{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git_ahead_and_behind{{ end }}",
                        "{{ if gt .Ahead 0 }}p:git_ahead{{ end }}",
                        "{{ if gt .Behind 0 }}p:git_behind{{ end }}"
                    ],
                    "foreground": "p:git_foreground",
                    "leading_diamond": "\ue0b6",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "branch_max_length": 25,
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "powerline",
                    "trailing_diamond": "\ue0b4"
                },
                {
                    "background": "#007800",
                    "background_templates": [
                        "{{ if gt .Code 0 }}#f1184c{{ end }}"
                    ],
                    "foreground": "#ffffff",
                    "leading_diamond": "<transparent,background>\ue0b0</>",
                    "properties": {
                        "always_enabled": true
                    },
                    "style": "diamond",
                    "template": " \uea6c ",
                    "trailing_diamond": "\ue0b0",
                    "type": "status"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "p:foreground",
                    "style": "plain",
                    "template": "\u279c ",
                    "type": "text"
                }
            ]
        }
    ]
}